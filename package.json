{
  "name": "hcqh",
  "version": "1.0.0",
  "description": "Trang Web Hop ca",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "live-server",
    "build:tags": "riot --ext rtg --m app gen/tags.js",
    "build:dist": "steal-tools build --config steal-config.js",
    "build": "npm run build:tags && npm run build:dist",
    "watch:tags": "riot --watch --ext rtg --m app gen/tags.js",
    "watch:dist": "steal-tools build --config steal-config.js --watch",
    "watch": "parallelshell \"npm run watch:tags\" \"npm run watch:dist\""
  },
  "author": "hiep",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^0.7.1",
    "parallelshell": "^1.2.0",
    "steal-tools": "^0.10.7"
  },
  "dependencies": {
    "jquery": "^2.1.4",
    "riot": "^2.2.4",
    "steal": "^0.10.5"
  }
}
